{
  "description": "A front-end library for managing browser cookies data.",
  "homepage": "https://Coditheck.github.io/advanced_cookie_manager",
  "name": "@prince-obrymec/advanced-cookie-manager",
  "author": "Obrymec - obrymecsprinces@gmail.com",
  "types": "./build/cookie.d.ts",
  "main": "./build/cookie.js",
  "sideEffects": false,
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodiTheck/advanced_cookie_manager/issues"
  },
  "repository": {
    "url": "https://github.com/CodiTheck/advanced_cookie_manager.git",
